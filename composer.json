{
    "name": "podcasthosting/audiowaveform-php-client",
    "description": "A PHP wrapper for easy access to the BBC project audiowaveform client.",
    "keywords": [
        "audio",
        "waveform",
        "mp3",
        "wav",
        "audiowaveform",
        "bbc"
    ],
    "homepage": "https://github.com/podcasthosting/audiowaveform-php-client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabio Bacigalupo",
            "email": "support@podcaster.de",
            "homepage": "https://www.podcaster.de"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "podcasthosting\\AudiowaveformClient\\": "src/"
        }
    },
    "require-dev": {
        "pestphp/pest": "^3.5"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "symfony/process": "^7.2"
    }
}
